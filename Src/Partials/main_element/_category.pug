- var img = "dist/Images/Category.png";
- var see = "";
- var arrow_left = "";

section.category 
  .container-fluid 
    .row
      .col-12
        include _headline.pug
        .swiper#category-slider
          .swiper-wrapper 
            .swiper-slide
              figure.item__box
                figcaption.item__box__caption
                    .item__box__caption__icon--one
                         img(
                         src="/dist/Images/(Traced).png",
                         alt="",
                         width="fit-content"
                         ) 
                    a.item__box__caption_content(href="javascript:void(0)") 
                         p.item__box__caption__content--text ارائه مشاوره آنلاین در زمینه ...
                    .item__box__caption__servises
                         button.btn.item__box__caption__servises__button مشاهده خدمات
                    
                         
